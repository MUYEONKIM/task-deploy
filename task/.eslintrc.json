{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-essential"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["vue"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-undef": "off",
    "vue/no-parsing-error": "off"
  }
}
